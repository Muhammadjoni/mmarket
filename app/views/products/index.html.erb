<ul>
  <% @products.each do |product| %>
    <li>
      <h2><%= product.name %></h2>
      <p><%= product.stock %></p>
      <p>Price: $<%= product.price %></p>
      <p><%= link_to "View Product", product_path(product) %></p>
    <%# <li><%= link_to product.name, product_path(product) %>
  <% end %>
</ul>